<h4>Session ID: <%= %></h4>

<h4>Announcements</h4>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Content</th>
        </tr>
    </thead>
    
    <tbody>
        <% @announcements.each do |announcement| %>
        <tr>
            <td><%= announcement.title %></td>
            <td><%= announcement.content %></td>
        </tr>
        <% end %>
    </tbody>
</table>

    

